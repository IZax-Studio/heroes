		<playeffect effect="/heroes/shellshock/ability_02/effects/gather_sound.effect" source="this_owner_entity" target="this_owner_entity" />
		<order command="stop" source="this_owner_entity" target="this_owner_entity" />
	</oninflict>
	
	<onframe>
		<teleport source="this_owner_entity" target="this_proxy_entity" noninterrupting="true" interpolate="true" />
		<hasmodifier name="shellshock_ability2_Gather" entity="this_proxy_entity" />
		<else>
			<expire />
			<spawnaffector name="Affector_Shellshock_Ability2" target="this_proxy_entity" proxy="this_owner_entity" />
		</else>
	</onframe>

</state>

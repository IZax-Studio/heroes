<?xml version="1.0" encoding="UTF-8"?>
<state
	name="State_Shellshock_Ability2_Ally"

	icon="icon2.tga"
	passiveeffect="effects/trail2.effect"

	stealthtype="unrevealable"

	disarmed="true"
	immobilized2="true"
	invulnerable="true"
	norender="true"
	unselectable="true"
	
	modifierkey="shellshock_ability2_Dragged"
>

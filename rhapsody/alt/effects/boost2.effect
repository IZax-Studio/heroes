					<particle
						turn="1"
						minscale="3"
						maxscale="7"
						minanglespeed="-200"
						maxanglespeed="200"
					>
						<model
							model="/shared/effects/models/circle_of_bones/model.mdf"
							anim="idle"
							scale=".25"
							pitch="90"
						>

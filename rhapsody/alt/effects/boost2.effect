									life="1000"
									spawnrate="30"
									material="/shared/effects/materials/arrow_trail.material"
									texposscale="1"
									texstretchscale="0"
									startparticlecolor="0"
									midparticlecolor="1"
									endparticlecolor="0"
									position="{position}"
									startminparticlelife="{life}"
									startmaxparticlelife="{life}"
									endminparticlelife="1"
									endmaxparticlelife="1" 
									texpostime="{life}"
								>
									<particle
										startsize="1"
										endsize=".5"
										startcolor="1 0 0"
										midcolor="1 1 1"
										endcolor="0 0 0"
									/>
								</trailemitter>
							</template>
							
							<instance name="streak" life="350" position="-5 0 0"/>
							<instance name="streak" life="450" position="-2 0 0"/>
							<instance name="streak" life="550" position="0 0 0"/>
							<instance name="streak" life="450" position="2 0 0"/>
							<instance name="streak" life="350" position="5 0 0"/>
							
							

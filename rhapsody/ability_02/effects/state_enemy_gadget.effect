					<template name="squares">
						<billboard
							life="{life}"
							position="{position}"
							material="/shared/effects/materials/whitesquare.material"
							startcolor="0 0 0"
							midcolor="{midcolor}"
							endcolor="0 0 0"
							midcolorpos=".2"
							size="40"
							lockup="1"
							lockright="1"
							pitch="90"
							depthbias="-25"
							loop="true"
						/>
					</template>
					
					<instance name="squares" position="0 0 5" life="1000" midcolor=".25"/>
					<instance name="squares" position="35 0 5" life="500" midcolor=".3 0 .2"/>
					<instance name="squares" position="-35 0 5" life="1000" midcolor=".3 0 .2"/>

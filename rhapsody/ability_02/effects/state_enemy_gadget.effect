				depthbias="-25"
				material="/shared/effects/materials/music_rings_overlay.material"
				position="0 0 12"
				startalpha="1"
				midalpha="1"
				endalpha="0"
				lockup="1"
				lockright="1"
				pitch="90"
				loop="true"
			/>
			

			<skeletonemitter
				offsetsphere="3"
				spawnrate="100"
				material="/shared/effects/materials/fireball_03_reveal_add.material"
				minparticlelife="350"
				maxparticlelife="450"
				gravity="-15"
				maxspeed="30"
				direction="0 0 1"
				angle="30"
				depthbias="-10"
				inheritvelocity="0.2"
				limitinheritvelocity="100"
				drag="0.02"
			>
				<particle
					minsize="40"
					maxsize="60"
					startscale=".5"
					endscale="1"
					startcolor=".5 .05 0"
					midcolor=".5 .05 0"
					endcolor="0"
					maxangle="360"
					minanglespeed="-60"
					maxanglespeed="60"
					startframe="1"
					midframe="0"
					endframe="1"
					midframepos=".25"
					param=".1"
				/>
			</skeletonemitter>

		</particlesystem>
	</definitions>
	<thread>
		<spawnparticlesystem instance="instance0" particlesystem="system0"/>
		<waitfordeath instance="instance0"/>
	</thread>
</effect>
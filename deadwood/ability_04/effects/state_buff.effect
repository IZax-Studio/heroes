					maxsize="13"
					startscale=".5"
					endscale="1"
					startcolor=".75 .15 0"
					midcolor=".5 0 0"
					endcolor="0"
					maxangle="360"
					minanglespeed="-60"
					maxanglespeed="60"
					startframe="1"
					midframe="0"
					endframe="1"

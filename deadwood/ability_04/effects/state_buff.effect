						startsize="{startsize}"
						midsize="{midsize}"
						endsize="{endsize}"
						startcolor="1 .5 .25"
						midcolor=".5 .05 0"
						endcolor="0"
						anchor="1"
						minanglespeed="-600"
						maxanglespeed="600"
					/>
				</twopointemitter>
			</template>
			
			<instance name="aura" bone_a="Bip01 R UpperArm" bone_b="Bip01 L UpperArm" spawnrate="25" gravity="-10" startsize="0" midsize="10" endsize="0"/>
			<instance name="aura" bone_a="Bip01 L UpperArm" bone_b="Bip01 L Forearm" spawnrate="25" gravity="-10" startsize="0" midsize="10" endsize="0"/>

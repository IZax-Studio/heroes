			<instance name="aura" bone_a="Bip01 L Thigh" bone_b="Bip01 L Calf" spawnrate="15" gravity="-3" startsize="0" midsize="20" endsize="0"/>
			<instance name="aura" bone_a="Bip01 R Thigh" bone_b="Bip01 R Calf" spawnrate="15" gravity="-3" startsize="0" midsize="20" endsize="0"/>
			<instance name="aura" bone_a="Bip01 L Calf" bone_b="Bip01 L Foot" spawnrate="15" gravity="-3" startsize="0" midsize="20" endsize="0"/>
			<instance name="aura" bone_a="Bip01 R Calf" bone_b="Bip01 R Foot" spawnrate="15" gravity="-3" startsize="0" midsize="20" endsize="0"/>
			<instance name="aura" bone_a="Bip01 L Foot" bone_b="Bip01 L Toe0" spawnrate="15" gravity="-3" startsize="0" midsize="20" endsize="0"/>
			<instance name="aura" bone_a="Bip01 R Foot" bone_b="Bip01 R Toe0" spawnrate="15" gravity="-3" startsize="0" midsize="20" endsize="0"/>
			
			<twopointemitter
				bone_a="Bip01 L Forearm"
				bone_b="Bip01 L Hand"
				offsetsphere="3"
				spawnrate="30"
				material="/shared/effects/materials/fireball_03_reveal_add.material"
				minparticlelife="350"
				maxparticlelife="450"
				gravity="-15"
				maxspeed="30"
				direction="0 0 1"
				angle="30"
				depthbias="-10"
				inheritvelocity="0.2"
				limitinheritvelocity="100"
				drag="0.02"
			>
				<particle
					minsize="10"
					maxsize="13"
					startscale=".5"
					endscale="1"
					startcolor=".75 .15 0"
					midcolor=".5 0 0"

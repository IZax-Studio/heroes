<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true" >
	<definitions>
		<particlesystem name="system0" space="world" scale=".65">	
		
							<sound
				volume="1"
				sample="/heroes/circe/ability_02/sounds/cast.wav"
				linearfalloff="true"
				falloffstart="700"		
				falloffend="1300" 
			/>
		
			<template name="cast_power">
				<controller bone="{bone}" scale=".4">
					<billboard

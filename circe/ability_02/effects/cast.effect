						color="0 1 1"
						life="500"
						startalpha="1"
						midalpha="1"
						endalpha="0"
						startsize="75"
						endsize="20"
						depthbias="-150"			
					/>

					<simpleemitter
						life="400"
						spawnrate="650"
						count="20"
						minparticlelife="230"
						maxparticlelife="1000"
						speed="100"
						drag="0.05"
						gravity="-1"
						offsetsphere="10 10 10"
						material="/shared/effects/materials/bad_healing_smoke_12.material"
					>
						<particle
							minangle="-180"
							maxangle="180"
							minanglespeed="-150"
							maxanglespeed="150"
							turn="1"
							startcolor="0 .1 .1"
							endcolor="0"
							size="25"
							minendscale="4"
							maxendscale="8"
							startalpha=".3"
							midalpha=".9"
							midalphapos="0.1"
							endalpha="0"

					endcolor="0"
					startframe="1"
					midframe=".1"
					endframe="1"
					startsize="150"
					endsize="300"
					maxangle="360"
					startalpha=".33"
					endalpha="0"
				/>
			</simpleemitter>

      </particlesystem>
   </definitions>
   <thread>
      <spawnparticlesystem instance="instance0" particlesystem="system0" />
      <waitfordeath instance="instance0" />
      <!-- <die /> -->
   </thread>
</effect>


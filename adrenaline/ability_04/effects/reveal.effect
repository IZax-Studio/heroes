				midcolor="0 .5 1"
				endcolor="0"
				material="/shared/effects/materials/corona_ground.material"
			/>
			
			<billboard
				life="800"
				startsize="0"
				midsize="350"
				endsize="400"
				startcolor="0 .5 1"
				endcolor="0"
				lockup="1"
				pitch="90"
				position="0 0 50"
				material="/shared/effects/materials/stomp_01.material"
			/>
			
			<billboard

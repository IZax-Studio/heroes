<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true" useentityeffectscale="1">
	<definitions>
<!--
		<modifier
			life="1500"
			name="transition"
			startalpha=".6"
			endalpha="1"
			startcolor="10 3 2"
			endcolor="1"
			midalphapos=".25"
		/>

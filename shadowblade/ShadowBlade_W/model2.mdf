	<anim name="ability_1" clip="sa/idle.clip" fps="550"/>

	<anim name="ability_2" clip="sa/ability_2.clip" fps="550" loop="true">
		<frameevent frame="1" cmd="PlaySoundLinear ability_2/sounds/impact.wav 1000 2500 .6"/>
                        <frameevent frame="1" cmd="PlaySoundLinear sa41.wav 1000 2500 .55"/>
	</anim>
	
	<anim name="ability_4" clip="sa/ability_4.clip" fps="550" loopbackframe="-1">
		<frameevent frame="1" cmd="PlaySoundLinear sa4.wav 1000 2500 .9"/>
	</anim>
		
	<anim name="attack_1" clip="sa/attack_2.clip" fps="550">
		<frameevent frame="11" cmd="PlaySoundLinear /heroes/hiro/sounds/attack_%.wav  400 2000 .65"/>
            <frameevent frame="3" cmd="StartEffect boost2.effect"/>
	</anim>
	
	<anim name="attack_2" clip="sa/attack_1.clip" fps="550">
		<frameevent frame="11" cmd="PlaySoundLinear /heroes/hiro/sounds/attack_%.wav  400 2000 .65"/>
            <frameevent frame="50" cmd="PlaySoundLinear sa4_%.ogg 1000 2500 .75"/>
            <frameevent frame="3" cmd="StartEffect boost3.effect"/>
            <frameevent frame="3" cmd="StartEffect aoe.effect"/>
	</anim>
		
	<anim name="knock_1" clip="sa/idle.clip" loop="true" loopbackframe="-1"/>
	
	<anim name="getup_1" clip="sa/idle.clip"/>		
			

<?xml version="1.0" encoding="UTF-8"?>
<projectile
	name="Projectile_Aluna_Ability3"
	
	speed="400"
	gravity="0"
	
	modelscale="1"
	model="/shared/models/invis.mdf"
	
	impacteffect=""
	traileffect="effects/shadow.effect"
	deatheffect=""
	
	canturn="true"
	flying="false"
	
	touchradius="80"
	maxtouchespertarget="1"

			<retarget target="stack_entity" />
		</compare>
		<else>
			<setactivemodifierkey entity="this_proxy_entity" name="" />
			<resettimer entity="this_proxy_entity" />
		</else>
	</onimpact>
	
	<altavatar key="Hero_Aluna.Alt"
		traileffect="../alt/ability_03/effects/shadow.effect"
	/>
	
	<altavatar key="Hero_Aluna.Alt2"

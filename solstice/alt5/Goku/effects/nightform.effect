<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true" useentityeffectscale="1">

	<definitions>


	
		<particlesystem name="system1" space="world" scale="1.75">


			<template name="bg_glow" >
				<simpleemitter
					timenudge="2000"
					bone="{bone}"
					spawnrate="10"
					minparticlelife="1000"
					maxparticlelife="1100"
					depthbias="8"
					material="/shared/effects/materials/light_rays.material"
				>
					<particle
						minstartsize="15"
						maxstartsize="30"
						minendsize="40"
						maxendsize="100"
						startcolor="0"
						midcolor="0 .05 0.1"
						endcolor="0"
						minanglespeed="-50"
						maxanglespeed="50"

						anchor="1"
					/>
				</simpleemitter>
			</template>
			

			<instance name="bg_glow" bone="_bone_head" />
			<instance name="bg_glow" bone="_bone_back" />
			<instance name="bg_glow" bone="_bone_center" />
			<instance name="bg_glow" bone="_bone_lefthand" />
			<instance name="bg_glow" bone="_bone_righthand" />					

						minsize="4"		
						maxsize="5"
						midsizepos=".2"		
						anchor="1"
					/>
				</simpleemitter> 


				<simpleemitter	
					bone="_bone_centerofchest"
					spawnrate="200"
					particlelife="400"
					depthbias="50"		
					acceleration="50"
					material="/shared/effects/materials/flare_02.material"
					offsetsphere="50 50 50"
					gravity="-25"
					position="0 0 -40"
				>
					<particle 
						minangle="-90"
						maxangle="90"
						minanglespeed="100"
						color=".5 .5 .5"
						maxanglespeed="0"
						startframe="0"
						endframe="1"				
						minsize="1"		
						maxsize="3"
						midsizepos=".2"		
						anchor="1"
					/>
				</simpleemitter>



			<simpleemitter
				bone="_bone_center"
				spawnrate="5"
				particlelife="1200"
				gravity="-15"
				speed="22.20"
				material="/shared/effects/materials/ember_reveal_blue.material"
				offsetsphere="10 10 10"
				position="0 0 -30"
				inheritvelocity=".025"
			>
				<particle
					startcolor="0 .74 1"
					midcolor="0 .74 1"
					endcolor="0 .74 1"
					midcolorpos=".75"
					startalpha=".1"
					midalpha=".15"
					endalpha="0"
					midalphapos=".9"
					minangle="-360"
					maxangle="360"
					minanglespeed="0"
					maxanglespeed="30"
					startsize="165"					
					endsize="20"
					anchor="1"
				/>
			</simpleemitter>

		
			
		</particlesystem>
	</definitions>

	<thread>
		<spawnparticlesystem instance="instance0" particlesystem="system0" />
		<spawnparticlesystem instance="instance1" particlesystem="system1" />
		<waitfordeath instance="instance0" />
		<waitfordeath instance="instance1" />
	</thread>

</effect>
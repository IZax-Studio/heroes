			<instance name="bg_glow" bone="Bone06" />
			<instance name="bg_glow" bone="Bone05" />

			<billboard
				startsize="300"
				endsize="700"
				position="0 0 70"
				material="/shared/effects/materials/flare_01.material"
				startcolor="0 .1 .3"
				endcolor="0 0 0"
				anchor="1"	
			/>

			<billboard
				size="300"
				position="0 0 70"
				material="/shared/effects/materials/flare_01.material"
				startcolor="0 .1 .3"
				endcolor="0 0 0"
				anchor="1"	
			/>		


				<simpleemitter	
					bone="_bone_centerofchest"
					spawnrate="50"
					particlelife="400"
					depthbias="50"		
					acceleration="50"
					material="/shared/effects/materials/star_03.material"
					offsetsphere="50 50 50"
					gravity="-25"
					position="0 0 -40"
				>
					<particle 
						minangle="-90"
						maxangle="90"
						minanglespeed="100"
						color="0 .5 1"
						maxanglespeed="0"
						startframe="0"

				offsetsphere="10 10 0"
				material="/shared/effects/materials/frost_burst.material"
			>
  				<particle 
					maxangle="360"
					minanglespeed="-6"
					maxanglespeed="6"
					startcolor="0 0 0"
					midcolor="0 .5 1"
					endcolor="0 0 0"
					startsize="0"
					endsize="100"
					startanchor="1"
					endanchor="1"

				minparticlelife="500"
				maxparticlelife="1000"
				minspeed="10"
				maxspeed="100"
				drag="0.01"
				inheritvelocity=".25"
				offsetsphere="10 10 10"
				gravity="2"
				material="/shared/effects/materials/flower2.material"
			>

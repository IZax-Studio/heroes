					startalpha="0"
					midalpha=".65"
					endalpha="0"
					minangle="0"
					maxangle="360"
					minanglespeed="-80"
					maxanglespeed="80"
					startsize="50"
					endsize="0"
					depthbias="-160"
					scale=".75"
				/>
				
				<particle
					color="0 .5 1"
					startalpha="0"
					midalpha=".65"
					endalpha="0"

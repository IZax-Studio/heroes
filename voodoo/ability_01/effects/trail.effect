					depthbias="-10"
					material="/shared/effects/materials/whitedot.material"
				/>

				<billboard
					bone="{bone}"
					material="/shared/effects/materials/blink.material"
					startcolor="1 1 1"
					midcolor="0"
					endcolor="0"
					maxroll="360"
					size="45"
					depthbias="-25"
					life="1000"
					directionalspace="local"
					rollspeed="-50"
					loop="true"
				/>
			</template>
			<instance name="glow" bone="_bone_weapon1"/>


			<trailemitter
				bone="_bone_weapon3"
				spawnrate="50"
				particlelife="800"
				texpostime="800"
			 	texposscale="0.0"
			 	texstretchscale=".4"
				position="3 0 0"
				material="/shared/effects/materials/arrow_trail.material"
			>
				<particle
					startcolor="0 .25 0"
					midcolor=".025 .1 .2"
					endcolor="0 0 0"
					startsize="0"
					midsize="10"
					endsize="10"
					midsizepos="0.2"
					scale=".75"
			 />
			</trailemitter>

			
			<trailemitter
				bone="_bone_weapon3"
				spawnrate="50"
				particlelife="800"
				texpostime="800"
			 	texposscale="0.0"
			 	texstretchscale=".4"
				position="-3 0 0"
				material="/shared/effects/materials/arrow_trail.material"
			>
				<particle
					startcolor="0 .25 0"
					midcolor=".025 .1 .2"
					endcolor="0 0 0"
					startsize="0"
					midsize="10"
					endsize="10"
					midsizepos="0.2"
					scale=".75"
			 />
			</trailemitter>
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			<template name="streak">
				<trailemitter
					name="trail02"
					spawnrate="60"
					startminparticlelife="{life}"
					startmaxparticlelife="{life}"
					endminparticlelife="1"
					endmaxparticlelife="1" 
					texpostime="{life}"
					texposscale="0"
					texstretchscale="1"
					position="{position}"
					material="/shared/effects/materials/arrow_trail.material"
				>
					<particle
						startcolor="{zstartcolor}"
						endcolor=".25 .25 .25"
						startsize="5"
						midcolorpos=".2"
						midsize="3"
						endsize=".5"
						scale=".75"
					/>
				</trailemitter>
			</template>
			
			<instance name="streak" life="350" zstartcolor="1 0 0" position="-5 0 0"/>
			<instance name="streak" life="450" zstartcolor="1 .5 0" position="-2 0 0"/>
			<instance name="streak" life="550" zstartcolor="0 1 0" position="0 0 0"/>
			<instance name="streak" life="450" zstartcolor="0 0 1" position="2 0 0"/>
			<instance name="streak" life="350" zstartcolor=".5 0 .5" position="5 0 0"/>
			
			
			
			
			
			
			
			

		
		
			<simpleemitter

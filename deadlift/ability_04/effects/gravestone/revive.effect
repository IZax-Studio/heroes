<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true">
	<definitions>
		<particlesystem name="system0" space="world" scale="1">
<sound
                forcestereo="true"
				linearfalloff="true" 
				falloffstart="1300"
				falloffend="1300"
				volume="1"
				sample="/heroes/deadlift/ability_04/sounds/ab4_vox.wav"
			/>          

		  <billboard
                life="1000"
                flare="1"
                material="/shared/effects/materials/light_rays.material"

                    minyaw="0"
                    maxyaw="360"
                    startscale="1"
                    endscale="2"
                >
                    <billboard
                       timenudge="1"
                        minlife="500"
                        maxlife="1000"
                        position="0 30 0"
                        lockup="true"
                        lockright="true"
                        pitch="-30"
                        material="/shared/effects/materials/light_rays2.material"
                        startcolor=".4 .3 .1"
                        midcolor=".2 0 0"
                        endcolor="0"
                        directionalspace="local"
                        startheight="40"
                        minendheight="400"
                        maxendheight="800"
                        startwidth="10"
                        minendwidth="10"
                        maxendwidth="80"
                    />
                </particle>
            </simpleemitter>


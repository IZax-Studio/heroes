                endheight="2000"
                startwidth="400"
                endwidth="20"
                lockup="true"
                startcolor=".5 .25 0"
                midcolor=".25 0 0"
                endcolor="0"
            />

            <simpleemitter
                timenudge="1"
                count="40"
                particlelife="2000"
                particledirectionalspace="local"
            >

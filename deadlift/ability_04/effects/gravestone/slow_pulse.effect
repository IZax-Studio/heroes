                    maxendsize="320"
                    startcolor=".25"
                    endcolor=".1 .1 0"
                    startframe="0"
                    endframe="1"
                    midframepos=".8"
                    lockup="true"
                    lockright="true"
                    pitch="90"
                    minscale=".9"
                    maxscale="1.1"
                />
            </simpleemitter>
			-->
        </particlesystem>

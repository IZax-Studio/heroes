				</particle>
			</simpleemitter>

			

			
			
			<simpleemitter
				life="70"
				spawnrate="300"
				position="0 30 0"
				minparticlelife="100"			
				maxparticlelife="200"	
				depthbias="-10"
				material="/shared/effects/materials/smoke_add.material"			
				offsetsphere="15 10 5"
				minspeed="1300"
				maxspeed="1500"
				angle="30"
				direction="0 -10 0"

				falloffend="2000"
				volume=".90"
				sample="\heroes\monkey_king\Elizabeth_Bathory\sounds/34_0_W_Fire.wav"
			/>
			
			<sound
				delay="300"
				linearfalloff="true" 
				falloffstart="0"
				falloffend="2000"
				volume=".2"
				sample="/heroes/witch_slayer/ability_04/sounds/shell_drop.wav"
			/>
		
			
			<simpleemitter
				spawnrate="1000"
				mincount="1"
				maxcount="3"
				particlelife="150"
				position="0 0 0"
				speed="0"
				direction="0 1 0"
				offsetsphere="0 15 0"
				minangle="0"
				maxangle="0"
			>
				<particle turn="true" minscale="1" maxscale="1.25">
					<model
						life="150"
						model="trailRosh/yuanhuan_ryan_0006.mdf"
						material="trailRosh/yuanhuan_ryan_0006.material"
						directionalspace="local"
						minsize="40"
						maxsize="50"
						startscale="0"
						midscale="4"
						endscale="5"
						maxroll="360"
						minrollspeed="-300"
						maxrollspeed="300"
						minyawspeed="-50"
						maxyawspeed="50"
						startcolor=".35"
						midcolor=".35"

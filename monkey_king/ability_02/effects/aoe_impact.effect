							midcolor=".4 .2 .4"
							endcolor=".4 .2 .4"
							midcolorpos=".5"
							startalpha="1"
							midalpha="1"
							endalpha="0"
							midalphapos="1"
						/>
						
					</twopointtrailemitter>
				</template>

				<instance name="trails" bone1="_bone_01" bone2="_bone_02"/>
				<instance name="trails" bone1="_bone_02" bone2="_bone_03"/>
				<instance name="trails" bone1="_bone_03" bone2="_bone_04"/>
				<instance name="trails" bone1="_bone_04" bone2="_bone_05"/>
				<instance name="trails" bone1="_bone_05" bone2="_bone_06"/>
				<instance name="trails" bone1="_bone_06" bone2="_bone_07"/>
				<instance name="trails" bone1="_bone_07" bone2="_bone_08"/>
				<instance name="trails" bone1="_bone_08" bone2="_bone_09"/>
				<instance name="trails" bone1="_bone_09" bone2="_bone_10"/>
				<instance name="trails" bone1="_bone_10" bone2="_bone_11"/>
				<instance name="trails" bone1="_bone_12" bone2="_bone_13"/>
				<instance name="trails" bone1="_bone_13" bone2="_bone_14"/>
				<instance name="trails" bone1="_bone_14" bone2="_bone_15"/>
				<instance name="trails" bone1="_bone_15" bone2="_bone_16"/>
				<instance name="trails" bone1="_bone_16" bone2="_bone_17"/>
				<instance name="trails" bone1="_bone_17" bone2="_bone_18"/>
				<instance name="trails" bone1="_bone_18" bone2="_bone_01"/>

			</model>

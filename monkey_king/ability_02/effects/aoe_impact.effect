<?xml version="1.0" encoding="UTF-8"?>
<effect name="">

	<definitions>
		<particlesystem name="system0" space="world" scale="2">
			<billboard
				
				life="500"
				startcolor=".5"
				midcolor="1 .5 .5"
				endcolor="0 0 0"
				startsize="25"
				midsize="120"
				endsize="155"
				midsizepos=".3"
				depthbias="-150"
				material="/shared/effects/materials/heart_ring.material"
				position="0 0 25"
				scale="1.2"
			/>
			<sound
				linearfalloff="true" 
				falloffstart="400"
				falloffend="2000"
				volume="1" 
				sample="\heroes\monkey_king\Elizabeth_Bathory\sounds/34_skin3_E_E_2_Fire.wav"
			/>

			<sound
				linearfalloff="true" 
				falloffstart="400"
				falloffend="2000"
				volume=".70" 

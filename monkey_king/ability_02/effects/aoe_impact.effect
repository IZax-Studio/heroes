				startsize="100"
				midsize="240"
				endsize="290"
				midsizepos=".3"
				depthbias="-30"
				position="0 0 20"
				material="/shared/effects/materials/corona_01.material"
				lockup="true"
				pitch="90"
			/> 
		 	
		
			
			
			
			<model
				name="ring"
				life="1000"
				model="/heroes/shadowblade/ability_01/effects/circle_rig/circle.mdf"
				anim="idle"
				yawspeed="360"
				startscale=".5"
				midscale="1.6"
				endscale="1.8"
				midscalepos=".2"
				position="0 0 50"
			>
				<template name="trails">
					<twopointtrailemitter
						life="800"
						name="trail01"
						bone_a="{bone1}"
						bone_b="{bone2}"
						spawnrate="50"
						particlelife="1800"
						startparticlealpha="1"
						endparticlealpha="0"
						startparticlecolor=".6 .6 .6"
						endparticlecolor=".6 .6 .6"
						midpartcilealphapos=".99"
						texpostime="1800"
						texposscale="0"
						texstretchscale="1"
						material="deadeye_logo.material"
					>

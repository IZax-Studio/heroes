			/>
			<sound
				linearfalloff="true" 
				falloffstart="400"
				falloffend="2000"
				volume=".35" 
				sample="/heroes/behemoth/ability_02/sounds/impact.wav" 
			/>
			
			<template name="ground" >
				<groundsprite
					position="{pos1}"
					minsize="130"
					maxsize="150"
					life="1000"
					startcolor=".6 .3 .1"
					midcolor=".6 .3 .1"
					endcolor="0"
					midcolorpos=".85"
					startalpha="1"
					midalpha="1"
					endalpha="0"
					midalphapos=".9"
					
					material="/shared/effects/materials/ice_reveal.material"
				/>
				<billboard
					position="{pos1}"
					minsize="200"
					maxsize="250"
					life="1000"
					startcolor=".5"
					midcolor=".3"
					endcolor="0"
					startalpha="1"
					midalpha=".5"
					endalpha="0"
					midalphapos=".8"
					roll="180"
					pitch="90"
					lockup="1"
					depthbias="-70"
					material="/shared/effects/materials/heart_04.material"
				/>
			</template>

			<terrainemitter
				life="1000"
				count="1"
				range="0"
				particlelife="8000"
			>
				<particle
					yaw="60"
				>
					<instance name="ground" pos1="0 0 0" yaw1="0" />

				</particle>	
			</terrainemitter>
			
			<billboard 
				scale="4"
				life="1000"
				startcolor=".5 .2 .5"
				midcolor=".5 .2 .5"
				endcolor="0"
				midcolorpos=".05"
				startalpha=".65"
				midalpha=".65"
				endalpha="0"
				midalphapos=".9"
				material="/shared/effects/materials/whitedot.material"
				lockup="1"
				lockright="1"
				pitch="90"
				depthbias="-25"
				position="0 0 1"
			/>

			<simpleemitter
				count="25"
				spawnrate="250"
				minparticlelife="100"
				maxparticlelife="200"
				speed="300"
				drag="0.10"
				material="/shared/effects/materials/whitedot.material"
				offsetsphere="10 10 10"
				position="0 0 35"
				depthbias="0"
			>
				<particle
					turn="1"
					heightdistort=".1"
					minstartwidth="1"
					maxwidth="7"
					minstartscale="5"
					maxstartscale="10"
					minendscale="0"
					maxendscale="4"
					startcolor=".25 .075 .25"
					endcolor="0 0 0"
					midcolorpos=".25"
				/>
			</simpleemitter>

			<billboard
				delay="0"
				life="250"
				position="0 0 0"
				material="/shared/effects/materials/refract_ripples.material"
				startalpha="0"
				midalpha="1"
				endalpha="0"
				startsize="250"
				endsize="350"
				startparam=".1"
				endparam="0"
				depthbias="-50"
				lockup="1"
				lockright="1"
				pitch="-90"
			/>

			<billboard
				delay="0"
				life="1000"
				position="0 0 0"
				material="/shared/effects/materials/refract_ripples.material"
				startalpha="0"
				midalpha=".5"
				endalpha="0"
				startsize="250"
				endsize="350"
				startparam=".1"
				endparam="0"
				depthbias="-50"
				lockup="1"
				lockright="1"
				pitch="-90"
			/>
			
			
			
			<terrainemitter
				life="1000"
				spawnrate="50"
				range="200"
				direction="0 0 1"
				angle="0"
				minspeed="100"
				maxspeed="200"
				drag=".02"
				particlelife="1000"
				material="/shared/effects/materials/spark_11.material"
			>
				<particle
					startcolor="1 .5 0"
					midcolor="1 0 0"
					endcolor="0"
					size="2"
					minscale=".5"
					maxscale="1"
					minanglespeed="-300"
					maxanglespeed="300"
				/>
			</terrainemitter>
			
			
			<billboard
				life="800"
				
				startcolor=".6 .3 .6"
				midcolor=".2 .05 .2"
				endcolor="0"
				midcolorpos=".3"
				

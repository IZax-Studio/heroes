						material="/heroes/frosty/pedestal.material"
						anim="idle"
						minspeed="1000"
						maxspeed="3000"
						direction="0 0 1"
						angle="60"
						drag="0.005"
						minrotationspeed="180"
						maxrotationspeed="360"
						gravity="100"
						collide="true"
						bounce=".5"
						reflect=".5"
						animpose="true"
						yawspeed="360"
						offsetsphere="600 600 0"
						startalpha="1"
						midalpha="1"
						endalpha="0"
					/>
				</particle>
			</simpleemitter>
			
			<groundsprite
				maxangle="360"
				material="/shared/effects/materials/ice_reveal_blend.material"
				minlife="750"

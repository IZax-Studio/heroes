<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true">
	<definitions>
		<particlesystem name="system0" space="world" scale="1.0">
		
			<sound
				norestart="true"
				channel="66034"
				life="2000"
				volume=".25" 
				sample="/heroes/ellonia/ability_02/sounds/iceexplode.wav"
				linearfalloff="true"
				falloffstart="100"
				falloffend="1300"
			/>
		
			<sound
				life="2000"
				minvolume=".2"
				maxvolume=".5" 
				sample="/heroes/frosty/ability_04/sounds/impact_%.wav"

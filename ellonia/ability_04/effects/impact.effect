					color=".6 .8 1"
					startframe="1"
					minendframe="0"
					maxendframe=".5"
					startalpha="0"
					midalpha=".25"
					endalpha="0"
					midalphapos=".25"
					size="50"
					minscale="4"
					maxscale="8"
					lockup="true"
					lockright="true"
					pitch="-90"
				/>

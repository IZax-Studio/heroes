				roll="90"
				size="150"
				depthbias="-30"
				frame="0.3"
				framespeed="1.95"				
			/>						
		
			
				<template name="eye_smoke">
					<billboard
						bone="{bone}"
						material="/shared/effects/materials/whitedot.material"
						color=".4 .4 .5"
						size="12"
						depthbias="-5"
					/>

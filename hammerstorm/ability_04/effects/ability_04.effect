				bone="_bone_weapon3"
				material="/shared/effects/materials/femme_lightning_06.material"				
				height="80"
				width="300"
				startcolor=".1 .4 .7"
				endcolor="0 0 0"
				lockup="1"
				position="0 0 150"
				roll="90"
				size="150"
				depthbias="-30"
				frame="0.5"
				framespeed="1.95"				
			/>	
		
			<billboard
				delay="120"
				life="800"				
				bone="_bone_weapon3"

						bone="{bone}"
						direction="{dir}"
						angle="0"
						speed="20"
						friction="100"
						gravity="-20"
						spawnrate="30"
						particlelife="600"
						material="{material}"
						texposscale="0"
						texstretchscale="1"
					>

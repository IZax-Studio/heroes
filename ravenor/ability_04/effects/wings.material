<?xml version="1.0" encoding="UTF-8"?>
<material>
	<parameters>
		<vec2 name="vUVScale" value=".1 .5" />
		<vec2 name="vUVScroll" value=".2 -0.125" />
		<float name="fBorderSharpness" value="10"/>
	</parameters>
	<phase
		name="color"
		vs="effect_fire_trail"
		ps="effect_fire_trail"
		srcblend="BLEND_SRC_ALPHA"
		dstblend="BLEND_ONE_MINUS_SRC_ALPHA"
		cull="CULL_NONE"
		translucent="true"
		alphawrite="true"
	>
		<sampler
			name="reveal"
			texture="/shared/effects/textures/light_scrolling_white.tga"
			repeat_u="true"
			repeat_v="true"
			mipmaps="true"
			fullquality="false"
			nocompress="false"
			filtering="true"
		/>
	</phase>
</material>

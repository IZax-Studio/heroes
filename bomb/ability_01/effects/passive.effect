			<instance  name="sound_pulse" delay="7000" volume=".8"/>
			<instance  name="sound_pulse" delay="8000" volume=".9"/>				
				
				
				
				
				
				
				
				</model>
				
			<model	
						directionalspace="local"
						
						scale="1.45"
						model="sphere/sphere.mdf"
						anim="idle"
						color="1 1 1"
						yawspeed="180"
						pitchspeed="-180"
						startalpha=".9"

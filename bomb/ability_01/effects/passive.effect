<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="0" useentityeffectscale="1">
	<definitions>
	
	
		<particlesystem name="system0" space="entity" scale="1">
	
	
				<model	
						directionalspace="local"
						
						scale="1.5"
						model=""

						color=".3 .15 .8"
						yawspeed="-180"
						pitchspeed="180"
						startalpha=".95"
			endalpha=".95"

						
						
						position="0 0 0"
				>
				
				
	
<sound
				sample="hadoken2.wav"
				linearfalloff="false"
				life="1500"
				falloffstart="0" 
				falloffend="2500" 
				volume="1"
			/>

			<template name="waves">	

				

 <billboard
				material="/shared/effects/materials/star_04.material"
				
				color=".5 .1 9"
				loop="1"
				life="750"
				
				position="0 0 0"
				size="130"
				depthbias="-250"			
			/>
			<billboard
				material="/shared/effects/materials/corona_07.material"
			

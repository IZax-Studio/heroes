			/>	

			
			</template>

			<template name="sound_pulse">	

			<sound
				delay="{delay}"
				sample="pulse.wav"
				linearfalloff="true"
				falloffstart="0" 
				falloffend="2500" 
				volume="{volume}"
			/>
			
			</template>
						
			<instance delay1="0" delay2="150" delay3="300" name="waves" endsize1="50" endsize2="100" endsize3="200"/>
			<instance delay1="1000" delay2="1150" delay3="1300" name="waves" endsize1="25" endsize2="150" endsize3="75"/>

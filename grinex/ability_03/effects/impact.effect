					<billboard
						life="400"
						startcolor=".75 0 0"
						endcolor="0"
						startsize="0"
						endsize="100"
						material="/shared/effects/materials/whitedot.material"
					/>
				</particle>
			</skeletonemitter>
			
			<skeletonemitter
				owner="target"
				minparticlelife="300"
				maxparticlelife="900"
				minspeed="100"
				maxspeed="500"
				drag=".075"
				gravity="-30"
				count="50"
				material="/shared/effects/materials/flare_05.material"
			>
				<particle
					size="4"
					startcolor="1 0 0"
					endcolor="0"
					maxangle="360"
				/>
			</skeletonemitter>
			
		</particlesystem>

	</definitions>

	<thread>
		
		<spawnparticlesystem instance="instance0" particlesystem="system0" />
		<wait duration="220" />
		<spawnparticlesystem instance="instance1" particlesystem="system1" />
		<waitfordeath instance="instance0" />
		<waitfordeath instance="instance1" />
	</thread>

</effect>
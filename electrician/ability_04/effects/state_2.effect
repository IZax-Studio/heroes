					minanglespeed="50"
					maxanglespeed="150"
					startcolor=".2 .4 1"
					midcolor=".2 .4 1"
					endcolor="0 0 0"
					midcolorpos=".8"
					startsize="1"
					midsize="10"
					maxendsize="15"
					startstickiness="0"
					endstickiness="1"

					startalpha="0"
					midalpha=".3"
					endalpha="0"
					midalphapos=".5"
					startstickiness="0"
					endstickiness="0"
				/>
			</simpleemitter>


		</particlesystem>
		
		<particlesystem name="system2" space="world" scale="3">
			<sound
				soundloop="true"
				linearfalloff="true"
				falloffstart="0"
				falloffend="2000"
				volume=".6"
				fadein="100"
				fadeout="400"
				sample="/heroes/electrician/ability_04/sounds/state.wav"
			/>
			<model
				bone="_bone_centerofchest"
				name="ring"
				model="/heroes/electrician/ability_04/effects/manaburn1_sphere/shield.mdf"
				anim="idle"
				yawspeed="0"
				scale=".5"
				color="1 .5 0"
			/>
			<billboard
				expirelife="300"
				material="/shared/effects/materials/refract_ripple.material"
				bone="_bone_centerofchest"
				loop="1"
				life="1000"
				startalpha=".3"
				midalpha=".6"
				endalpha=".3"
				position="0 0 10"
				size="70"
				depthbias="-150"
				param=".01"
			/>

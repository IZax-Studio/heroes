					</trailemitter>
					<trailemitter
						bone="{bone}"
						spawnrate="200"
						angle="0"
						material="fire_scrolling_trail.material"
						speed="1000"
						direction="0 1 0"
						particlelife="200"
						texpostime="200"
						texposscale="0"
						texstretchscale="1"
						startparticlecolor="1"
						endparticlecolor="0"
					>
						<particle
							color=".25 .1 1"
							startsize="0"
							midsize="30"
							endsize="30"
							midsizepos=".1"
						/>
					</trailemitter>
				</template>
					
				<instance name="trail" bone="_bone_fx_01"/>
				<instance name="trail" bone="_bone_fx_02"/>
				<instance name="trail" bone="_bone_fx_03"/>
					
			</model>
			
			
		</particlesystem>
	</definitions>
	<thread>
		<spawnparticlesystem instance="instance0" particlesystem="system0" />
		<waitfordeath instance="instance0" />
	</thread>
</effect>
				maxyaw="360"
				yawspeed="100"
				startscale="1"
				endscale="0"
			>
				<template name="trail">
					<billboard
						bone="{bone}"
						material="../../omen/custom/ef_ww_spot_01.material"
						startcolor=".5 .25 .75"
						endcolor="0"
						size="50"
						rollspeed="100"
					/>
					<trailemitter
						bone="{bone}"
						spawnrate="200"
						speed="600"
						direction="0 1 0"
						angle="0"
						material="../../omen/custom/ef_x_line_05_solid_931c1451.material"
						particlelife="200"
						texpostime="200"
						texposscale="0"
						texstretchscale="1"
						startparticlecolor="1"
						endparticlecolor="0"
					>

<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="1" useentityeffectscale="1">
	<definitions>   
		<particlesystem name="system0" space="world" scale="1" >
		
			<sound
				norestart="true"
				linearfalloff="true" 
				falloffstart="1000"
				falloffend="1800"
				volume="1"
				sample="../../omen/sounds/ab2_shield_start.wav"
			/>
			
			<sound
				norestart="true"
				linearfalloff="true" 
				falloffstart="0"
				falloffend="1300"
				volume="1"
				sample="../../omen/sounds/ab2_vox_%.wav"
			/>

			<model
				timenudge="1"
				position="0 0 120"
				expirelife="500"

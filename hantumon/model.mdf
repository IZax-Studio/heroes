		<frameevent frame="108" cmd="StartEffect /shared/effects/footstep_r_#GetTerrainType()#.effect"/>
		<frameevent frame="108" cmd="StartEffect /shared/effects/footstep_l_#GetTerrainType()#.effect"/>
		<frameevent frame="126" cmd="StartEffect /shared/effects/footstep_l_#GetTerrainType()#.effect"/>
	</anim>

	<anim name="death_1" clip="Death Alternate.clip" fps="550" loop="true" loopbackframe="-1">
		<frameevent frame="1" cmd="PlaySoundLinear s/d%.wav 400 1800 .6"/>
		<frameevent frame="330" cmd="StartEffect death.effect"/>
		<frameevent frame="990" cmd="PlaySoundLinear /heroes/hantumon/sounds/attack_1.wav 400 1800 .35"/>
		<frameevent frame="995" cmd="PlaySoundLinear /heroes/hantumon/sounds/attack_2.wav 400 1800 .35"/>
		<frameevent frame="1000" cmd="PlaySoundLinear /heroes/hantumon/sounds/attack_3.wav 400 1800 .35"/>
		<frameevent frame="1005" cmd="PlaySoundLinear /heroes/hantumon/sounds/attack_1.wav 400 1800 .35"/>
		<frameevent frame="1010" cmd="PlaySoundLinear /heroes/hantumon/sounds/attack_2.wav 400 1800 .35"/>
		<frameevent frame="1290" cmd="PlaySoundLinear /shared/sounds/impacts/flesh_%.wav 400 1800 .35"/>
		<frameevent frame="1320" cmd="PlaySoundLinear /heroes/devourer/sounds/move_%.wav 400 1800 .55"/>		

	</anim>
	<anim name="bored_1" clip="s.clip" fps="550" loop="false"/>
	<anim name="taunt_1" clip="Spell 3 Alternate.clip" fps="550" loop="false"/>
	
	<anim name="knock_1" clip="Death Alternate.clip" loop="true" fps="550" loopbackframe="-1"/>
	
	<anim name="getup_1" clip="Stand Alternate.clip"/>	

<?xml version="1.0" encoding="UTF-8"?>
<effect deferred="true">
	<definitions>
		<particlesystem name="system0" space="world" scale="1">
			<sound
				volume="1" 
				sample="\heroes\witch_slayer\Shiro_sounds/215_1_R_Trace.wav"
				linearfalloff="true"
				falloffstart="0"
				falloffend="1300"
			/>
			<light
				color="1 0 0"
				falloffstart="0"
				falloffend="1055"
			/>

		<model
			
				model="/heroes/vanya/set_ascension/ability_3/effects/shock_wave/model.mdf"
				material="\shared\effects\materials/fire_scrolling_beam.material"
				scale="2"
				color="1 .0 0"
				position="0 100 0"
				pitch="90"
			/>
			
		

			<simpleemitter
				count="1"
				particlelife="800"
				material="/shared/effects/materials/refract_swell.material"
				particledirectionalspace="local"
			>
